{
	"name": "@flit/sveltekit-adapter-cdk",
	"version": "1.18.0",
	"description": "SvelteKit adapter for AWS CDK",
	"keywords": [
		"aws",
		"awscdk",
		"svelte",
		"sveltekit"
	],
	"homepage": "https://github.com/vierroth/sveltekit-adapter-cdk",
	"bugs": "https://github.com/vierroth/sveltekit-adapter-cdk/issues",
	"repository": {
		"type": "git",
		"url": "https://github.com/vierroth/sveltekit-adapter-cdk.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Vierroth GmbH",
		"url": "https://github.com/vierroth"
	},
	"contributors": [
		{
			"name": "Luis Vierroth",
			"url": "https://github.com/p-mercury"
		}
	],
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"./dist",
		"./LICENSE.txt",
		"./README.md"
	],
	"workspaces": [
		"./example"
	],
	"scripts": {
		"build": "npm run prepack",
		"prepack": "rm -rf ./dist && tsc -b"
	},
	"prettier": {
		"plugins": [
			"prettier-plugin-packagejson"
		],
		"useTabs": true
	},
	"dependencies": {
		"@sveltejs/kit": "^2.43.5",
		"@types/aws-lambda": "^8.10.152",
		"@types/node": "^24.6.0",
		"aws-cdk-lib": "^2.218.0"
	},
	"devDependencies": {
		"prettier": "^3.6.2",
		"prettier-plugin-packagejson": "^2.5.19",
		"prettier-plugin-svelte": "^3.4.0",
		"typescript": "^5.9.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"stability": "stable"
}
